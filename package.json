{
  "name": "rvt2-analyzer",
  "description": "A web interface for the rvt2",
  "version": "3.1.1",
  "author": "Juanvi Vera <jvera@incide.es>",
  "license": "GPL",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "docs": "node_modules/.bin/jsdoc -c .jsdoc.json -R README.md src"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "epic-spinners": "^1.1.0",
    "eslint-loader": "^3.0.2",
    "file-icons-js": "^1.0.3",
    "js-file-download": "^0.4.9",
    "vue": "^2.5.11",
    "vue-router": "^3.1.2",
    "vuetify": "^2.1.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.6.95",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "babel-eslint": "^10.0.2",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "eslint-plugin-vuetify": "^1.0.0-beta.5",
    "fibers": "^4.0.2",
    "jsdoc": "^3.6.3",
    "jsdoc-vuejs": "^3.0.4",
    "minami": "^1.2.3",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.4.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "vuetify",
      "prettier",
      "vue"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "plugin:prettier/recommended",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "Console|[iI]gnored"
        }
      ],
      "vuetify/no-deprecated-classes": "error",
      "vuetify/grid-unknown-attributes": "error",
      "vuetify/no-legacy-grid": "error"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
