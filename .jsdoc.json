{
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-vuejs"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": "\\.(vue|js)$"
  },
  "opts": {
      "access": "all",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "templates.default.outputSourceFiles": false,
      "template": "node_modules/minami"
  }
}
